{
  "name": "squigg/azure-queue-laravel",
  "description": "Laravel 5 Queue Driver for Microsoft Azure Storage Queue",
  "keywords": [
    "laravel",
    "azure",
    "storage",
    "queue",
    "microsoft"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Steve Strugnell",
      "email": "github@squigg.co.uk"
    }
  ],
  "require": {
    "php": "^7.1",
    "illuminate/queue": ">=5.7,<5.8",
    "microsoft/azure-storage-queue": ">=1.0.0,<1.2"
  },
  "require-dev": {
    "phpunit/phpunit": "~7.0",
    "orchestra/testbench": "3.7.*",
    "mockery/mockery": "~1.0",
    "satooshi/php-coveralls": "~2.0"
  },
  "autoload": {
    "psr-4": {
      "Squigg\\AzureQueueLaravel\\": "src/"
    }
  },
  "minimum-stability": "stable",
  "extra": {
    "laravel": {
      "providers": [
        "Squigg\\AzureQueueLaravel\\AzureQueueServiceProvider"
      ]
    }
  }
}
